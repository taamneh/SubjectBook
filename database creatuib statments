select subject_id,session_no, session_name, signal_loc from subject, session where study_study_id =1 AND subject.subject_seq = session.subject_seq order by subject_id, session_no ;

select distinct(subject_id) from subject, session where study_study_id =1 AND subject.subject_seq = session.subject_seq order by subject_id;
select session_no, session_name, signal_loc from subject, session where study_study_id =1 AND subject.subject_seq = session.subject_seq AND subject_id ='S001' order by subject_id, session_no ;